function [T]=find_temperature_profile_conduction(input)
i0 = input(1);
h = input(2);
l = input(3);
m = input(4);
alpha = input(5);
Re = input(6);
Pr = input(7);
D = input(8);
n = input(9);
t = D;
k = (h*l*l)/(t*m*m);
Tr=298;
Tinf=298;
if (Re<2000)
Nu=find_nusselt_laminar(Re,Pr,D,l);
else
Nu=find_nusselt_turbulent(Re,Pr);
end
tsnum=(1/(m*m))-((Nu/m)*Tr*coth(m/2));
tsden=1-(Nu*coth(m/2)/m);
Ts=tsnum/tsden;
firstterm=(i0*l*l)*(1-((2/pi)*find_integral(alpha,l)))/(k*t);
x=0;
for i=1:1:n
    xbar = x/l;
    secondterm=((Nu/m)*(Ts-Tr)*(cosh(m*(xbar-0.5))/sinh(m/2)))+(1/(m*m));
    T(i)=Tinf + firstterm*secondterm;
    x = x + (l/(n-1));
end